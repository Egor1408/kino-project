{
  "extends": ["airbnb/base"],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    // "semi": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": ["error", 150, {
      "ignoreUrls": true
    }],
    "no-param-reassign": "off",
    "no-unused-vars": "off",
    "linebreak-style": 0,
    "no-return-await": "off",
    "no-underscore-dangle": "off"
  }
}